{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parser": "@babel/eslint-parser",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "babelOptions": {
            "configFile": "./babel.config.js"
          }
    },
    "rules": {
        "indent": ["warn", 4],
        "max-len": ["warn", 500]
    }
}
